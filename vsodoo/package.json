{
	"name": "vsodoo",
	"displayName": "vsodoo",
	"publisher": "Block-Cat",
	"description": "vscode extension for odoo development.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.32.0"
	},
	"icon": "images/odoo.png",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:__manifest__.py",
		"onCommand:extension.odoo"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.odoo",
				"title": "Odoo Test"
			}
		],
		"snippets": [
			{
				"language": "python",
				"path": "./snippets/mainfest.json"
			},
			{
				"language": "python",
				"path": "./snippets/model.json"
			},
			{
				"language": "python",
				"path": "./snippets/function.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
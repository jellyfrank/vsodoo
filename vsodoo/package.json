{
	"name": "vsodoo",
	"displayName": "vsodoo",
	"publisher": "Qingdao Ohm Network Technology",
	"description": "vscode extension for odoo development.",
	"icon": "vsodoo.png",
	"repository": {"type": "git","url": "https://github.com/jellyfrank/vsodoo"},
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.94.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:__manifest__.py",
		"onCommand:extension.odoo"
	],
	"main": "./out/extension.js",
	"contributes": {
		"snippets": [
			{
				"language": "python",
				"path": "./snippets/manifest.json"
			},
			{
				"language": "python",
				"path": "./snippets/model.json"
			},
			{
				"language": "python",
				"path": "./snippets/function.json"
			},
			{
				"language": "xml",
				"path": "./snippets/xml.json"
			},
			{
				"language": "xml",
				"path": "./snippets/form_view.json"
			},
			{
				"language": "xml",
				"path": "./snippets/tree_view.json"
			},
			{
				"language": "python",
				"path": "./snippets/tests.json"
			},
			{
				"language": "javascript",
				"path": "./snippets/js.json"
			},
			{
				"language": "python",
				"path": "./snippets/controller.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"tslint": "^5.12.1",
		"typescript": "^3.4.5",
		"vscode": "^1.94.0"
	}
}
